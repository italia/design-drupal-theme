{
  "name": "bootstrap_italia",
  "version": "2.4.3",
  "description": "'A Drupal theme which implements the Italia Design System'",
  "main": "src/js/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=v16.0.0"
  },
  "scripts": {
    "test": "npm run test-htmlprofiler",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --config ./webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --config ./webpack.prod.js",
    "watch:dev": "cross-env NODE_ENV=development webpack --watch --progress --config ./webpack.dev.js",
    "watch:prod": "cross-env NODE_ENV=production webpack --watch  --progress --config ./webpack.prod.js",
    "hot": "cross-env NODE_ENV=development webpack serve --config ./webpack.hot.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.drupal.org:project/bootstrap_italia.git"
  },
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "author": "Bootstap_italia theme developer - See https://www.drupal.org/node/3094960/committers",
  "license": "GPL-2.0-only",
  "devDependencies": {
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.2",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "semver": "^7.5.1",
    "style-loader": "^3.3.3",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^4.0.0",
    "webpack": "^5.85.1",
    "webpack-cli": "^5.1.3",
    "webpack-dev-server": "^4.15.0",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "bootstrap": "5.2.3",
    "bootstrap-italia": "2.4.3"
  }
}
