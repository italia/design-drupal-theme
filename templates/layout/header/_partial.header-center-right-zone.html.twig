<div class="it-right-zone">
  {% if page.header_center_social %}
    {{ page.header_center_social }}
  {% else %}
    {% include '@bootstrap_italia_components/social/social.twig' %}
  {% endif %}
  {% if page.header_center_search %}
    <div class="it-search-wrapper">
      <span class="d-none d-md-block">Cerca</span>
      <a class="search-link rounded-icon" aria-label="Cerca" href="#" data-target="#search-modal" data-toggle="modal">
        {% include '@bootstrap_italia_components/icon/icon.twig' with { name: 'it-search' } %}
      </a>
    </div>
  {% endif %}
</div>
